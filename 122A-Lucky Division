#include <bits/stdc++.h>
using namespace std;
#define ll long long

/*
 
                    AUTHOR        : NEEL SHETH
                                    AHMEDABAD UNIVERSITY

                    CODEFORCES    : neelxdxd
                    CODECHEF      : neelxdxd

*/

int main(){
    ll n;
    cin>>n;
    int ar[12]={4,7,44,47,74,44,444,447,477,744,774,777};
    int flag=0;
    for(int i=0;i<12;i++){
        if(n%ar[i]==0){
            cout<<"YES"<<endl;
            flag=1;
            break;
        }
    }if(flag==0){
        cout<<"NO"<<endl;
    }
    return 0;
}
